CFLAGS = $(STD_C_FLAGS) $(PLATFORM_C_FLAGS)
C_PLUS_FLAGS = -Wall $(STD_C_PLUS_FLAGS) -fno-implicit-templates -DNEW_PROC -DALLOC_DEBUG -DWANT_DC_PM

SRC = timer_manager.C daemon_core.C daemon_core_main.C test_daemon_core.C instantiate.C condor_ipverify.C
OBJ = timer_manager.o daemon_core.o daemon_core_main.o instantiate.o condor_ipverify.o

LIB = ../condor_c++_util/cplus_lib.a  \
	 ../condor_classad/expr_lib.a ../condor_util_lib/util_lib.a SocketLib $(AUTH2_LIB)

all_target(daemon_core.a)
library_target(daemon_core.a,$(OBJ))
c_plus_target(testit,test_daemon_core.o daemon_core.a,$(LIB))
tags_target($(SRC),$(NULL))
depend_target($(SRC) $(LIB))
release:
stripped:
