PROGRAMS = 	uniq_pid_command

all_target($(PROGRAMS))

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = $(STD_LIBS)

OBJ = uniq_pid_tool_main.o


/* C++ Tools */
c_plus_target(uniq_pid_command,$(OBJ),$(LIB))

release_target(uniq_pid_command,sbin,EXECUTABLE_MODE)
strip_target(uniq_pid_command,sbin,EXECUTABLE_MODE)
static_target(uniq_pid_command,$(OBJ),$(LIB),$(STATIC_DIR)/sbin)

release_link_target($(RELEASE_DIR)/sbin/uniq_pid_command,sbin,uniq_pid_midwife)
