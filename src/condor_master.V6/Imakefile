NAME = condor_master
all_target($(NAME))

CFLAGS = $(STD_C_FLAGS) 
C_PLUS_FLAGS =  $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS) $(CONFIGURE_GSOAP_LDFLAGS)

soapobjs(master)

#if HAVE_DLOPEN
PLUGIN_OBJ = MasterPlugin.o
#endif

%-plugin.so: %.C
   $(CPlusPlus) $(C_PLUS_FLAGS) -shared -fPIC $< -o $@

OBJ = master.o daemon.o $(SOAP_OBJS) $(PLUGIN_OBJ)

public_c_plus_target($(NAME),sbin,$(OBJ),$(LIB))

html:

gen_soapfiles(master,condorMaster)

testbin:: all
