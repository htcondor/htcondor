CONFIGURE_GSOAP_LDFLAGS = @GSOAP_LDFLAGS@
CONFIGURE_POSTGRESQL_LDFLAGS = @POSTGRESQL_LDFLAGS@
CONFIGURE_X11_CFLAGS = @X11_CFLAGS@
CONFIGURE_X11_LDFLAGS = @X11_LDFLAGS@
CONFIGURE_X11_LIBS = @X11_LIBS@

LEX = @LEX@
LEXLIB = @LEXLIB@
YACC = @YACC@
STRIP = @STRIP@
TRUE = @TRUE@
JAVAC = @JAVAC@
