OBJCOPY_CMD = @ObjcopyCmd@
OBJCOPY_KEEPDEBUG_FLAG = @OBJCOPY_KEEPDEBUG_FLAG@
OBJCOPY_STRIPUNNEEDED_FLAG = @OBJCOPY_STRIPUNNEEDED_FLAG@
OBJCOPY_DEBUGLINK_FLAG = @OBJCOPY_DEBUGLINK_FLAG@



RPM_CMD = @RpmCmd@

TAR_CMD = @TarCmd@

CP_CMD = @CpCmd@

TMP_DIR = @TMPDIR@

PURE_CACHE_DIR = @pure_cache_dir@

#define HAS_STATIC @_cv_has_static@

GlobusFlavor = @globus_flavor@
